<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<constant name="struts.devMode" value="true" />

	<package name="struts2" extends="struts-default">
	
		<action name="index">
			<result>/initSession.jsp</result>
		</action>


		<action name="*Action" class="struct.actions.MetodosAction" method="{1}">
			<result name="input">/InitSession.jsp</result>
			<result name="portal">/portal.jsp</result>
			<result name="editInfo">/editInfoPersonal.jsp</result>
	    	<result name="editLoginPass">/editLoginPass.jsp</result>
	    	<result name="success_update">/successUpdate.jsp</result>
	    	<result name="displayWishList">/wishlist.jsp</result>
	    	<result name="displayShoppingCart">/shoppingCart.jsp</result>
	    	<result name="displayBuyHistory">/buyHistory.jsp</result>
	    	<result name="displayUsersList">/usersList.jsp</result>
			<result name="displayProductsList">/productsList.jsp</result>
			<result name="displayConsolesList">/consolesList.jsp</result>
			<result name="index">/principal.jsp </result>
			<result name="success">/prueba.jsp </result>
			<result name="editProduct">/editInfoProduct.jsp </result>
			<result name="editClient">/editInfoUser.jsp </result>
			<result name="registry_successful">/successfulRegistry.jsp</result>
			<result name="catalog">/Catalog.jsp </result>
			<result name="productdetail">/productDetail.jsp</result>
		</action>

	</package>
	
</struts>